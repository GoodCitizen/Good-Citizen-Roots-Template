{
  "name": "<%= S.dasherize(name) %>",
  "description": "<%= description %>",
  "version": "0.1.0",
  "author": "John Peele <john@wearegoodcitizen.com>",
  "devDependencies": {
    "chai": "2.3.x",
    "chai-as-promised": "5.0.x",
    "mocha": "2.2.x",
    "rimraf": "2.3.x",
    "roots": "^3.1.0",
    "roots-util": "^0.1.0",
    "git-scripts": "^0.2.1",
    "surge": "^0.17.2"
  },
  "dependencies": {
    "autoprefixer-stylus": "0.5.x",
    "axis": "0.5.x",
    "coffee-script": "1.10.x",
    <% if (postcssPlugins["cssnext"] && postcssPlugins["cssnext"].checked) { %>
    "cssnext": "1.8.x",
    <% } %>
    "css-pipeline": "0.3.x",
    "jade": "1.x",
    <% if (grid == true) { %>
    "jeet": "6.1.x",
    <%} -%>
    "js-pipeline": "0.2.x",
    "jstransformer-marked": "1.0.x",
    <% if (grid == true) { %>
    "lost": "6.6.x",
    <%} -%>
    <% if (postcssPlugins["system"] && postcssPlugins["system"].checked) { %>
    "postcss-system": "0.0.x",
    <% } %>
    <% if (postcssPlugins["postcss font pack"] && postcssPlugins["postcss font pack"].checked) { %>
    "postcss-font-pack": "1.0.x",
    <% } %>
    <% if (postcssPlugins["postcss import"] && postcssPlugins["postcss import"].checked) { %>
    "postcss-import": "7.1.x",
    <% } %>
    "poststylus": "0.2.x",
    <% if (yaml == true) { %>"roots-yaml": "0.0.x",<%} -%>
    <% if (postcssPlugins["rucksack"] && postcssPlugins["rucksack"].checked) { %>
    "rucksack-css": "0.8.x",
    <% } %>
    "rupture": "0.6.x",
    "stylus": "0.52.x"
  },
  "homepage": "https://bitbucket.org/goodcitizen/<%= S.slugify(name) %>",
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/goodcitizen/<%= S.slugify(name) %>.git"
  },
  "git": {
    "scripts": {
      "pre-push": "surge --project ./public --domain <%= S(name).dasherize().value() %>.surge.sh"
    }
  }
}
